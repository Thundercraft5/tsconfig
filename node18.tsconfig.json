{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Node 18",
	"extends": "./esm-node.tsconfig.json",
	"compilerOptions": {
		"lib": [
			"ES2021",
			"ESNext"
		],
		"module": "NodeNext",
		"moduleResolution": "node",
		"target": "esnext",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"useDefineForClassFields": true
	}
}