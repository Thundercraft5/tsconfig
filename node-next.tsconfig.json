{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Node Next",
	"extends": "./esm-node.tsconfig.json",
	"compilerOptions": {
		"lib": [
			"ESNext",
		],
		"module": "ESNext",
		"moduleResolution": "bundler",
		"target": "esnext",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"useDefineForClassFields": true
	}
}